---
import type { PropsWithClassName } from "@/types"

type Props = PropsWithClassName & {
  width?: number
  height?: number
  color?: string
}

const { className, width = 32, height = 32, color = "black" } = Astro.props
---

<svg class={className} width={width} height={height} viewBox="0 0 33 32" fill="none" style="transform: scale(1.05);">
  <rect x="11.6904" y="9.85693" width="3.50977" height="12.7229" fill={color}></rect>
  <rect x="18.2715" y="9.85693" width="3.50977" height="12.7229" fill={color}></rect>
  <circle cx="16.4543" cy="15.9817" r="15.0004" stroke={color} stroke-width="1.30438"></circle>
</svg>
